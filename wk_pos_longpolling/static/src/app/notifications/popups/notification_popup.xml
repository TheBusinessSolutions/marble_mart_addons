<templates id="template" xml:space="preserve">

    <t t-name="NotificationPopUp">

        <div class="popup popup-notification">
            <div class="modal-header">
                <h4 class="modal-title title">
                    <t t-esc=" props.title || 'Message' " />
                </h4>
            </div>
            <main class="modal-body">
                <t t-esc="  props.body || '' " />
            </main>
            <div class="footer modal-footer justify-content-end">
                <div class="button cancel btn btn-primary" t-on-click="cancel" style="width:130px">
                    OK
                </div>
            </div>
        </div>

    </t>

</templates>